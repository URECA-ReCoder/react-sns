{
    "compilerOptions": {
      "allowJs": true,
      "target": "ES6",
      "outDir": "./dist",
      "moduleResolution": "Node",
      "lib": ["ES2015", "DOM", "DOM.Iterable"],
      "jsx": "preserve",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "jsxImportSource": "@emotion/react",
      "noImplicitAny": true,
    },
    "include": ["./src/**/*"],
    "exclude": ["node_modules", "dist"]
  }